<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Page 2"
             x:Class="Xamarin.Tuan2.SecondPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Take me to previous page"
                    TextColor="White"
                    BackgroundColor="Black"
                    Clicked="Button_OnClicked_Back"/>
            <Button Text="Take me to Root Page"
                    TextColor="White"
                    BackgroundColor="Black"
                    Clicked="Button_OnClicked_RemoveAll"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>